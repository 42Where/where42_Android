<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/help_popup_width"
    android:layout_height="@dimen/help_popup_height"
    android:background="@drawable/center_popup_background"
    >
    <!--    android:layout_width="@dimen/newgroup_layout_width"-->
    <!--    android:layout_height="@dimen/newgroup_layout_height"-->
    <!--    android:paddingTop="@dimen/newgroup_layout_padding_top_bottom"-->
    <!--    android:paddingBottom="@dimen/newgroup_layout_padding_top_bottom"-->
    <!--    android:paddingStart="@dimen/newgroup_layout_padding_start_end"-->
    <!--    android:paddingEnd="@dimen/newgroup_layout_padding_start_end"-->


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />



    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/guideline1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="어디있니?"
        android:textColor="#132743"
        android:textSize="@dimen/help_popup_font"
        android:fontFamily="@font/gmarketsans_bold" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.18" />


    <TextView
        android:id="@+id/middle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/guideline2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="안녕하세요! 42서울 친구 자리 찾기 서비스 '어디있니?'입니다."
        android:textColor="#132743"
        android:textSize="@dimen/help_popup_font_small"
        android:textAlignment="center"
        android:fontFamily="@font/gmarketsans_medium"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.277" />


    <TextView
        android:id="@+id/detail1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/guideline3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="'어디있니?' 서비스에서는 원하는 카뎃의 자리를 검색해 볼 수 있고, 카뎃들을 친구로 등록하여 여러명의 자리를 한 번에 모아볼 수 있습니다."
        android:textColor="#132743"
        android:textSize="@dimen/help_popup_font_small"
        android:textAlignment="center"
        android:fontFamily="@font/gmarketsans_medium"
        android:paddingRight="@dimen/list_padding_top_bottom"
        android:paddingLeft="@dimen/list_padding_top_bottom"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.51" />

<!--    <TextView-->
<!--        android:id="@+id/noiton"-->
<!--        android:layout_width="100dp"-->
<!--        android:layout_height="30dp"-->
<!--        android:background="@drawable/submit_button"-->
<!--        app:layout_constraintTop_toBottomOf="@id/guideline11"-->
<!--        app:layout_constraintStart_toStartOf="parent"-->
<!--        app:layout_constraintEnd_toEndOf="parent"-->
<!--        android:text="Noition 보기"-->
<!--        android:textColor="@color/white"-->
<!--        android:textSize="13sp"-->
<!--        android:fontFamily="@font/gmarketsans_medium"-->
<!--       -->
<!--        android:paddingRight="10dp"-->
<!--        android:paddingLeft="10dp"/>-->


    <android.widget.Button
        android:id="@+id/noiton"
        android:layout_width="110dp"
        android:layout_height="30dp"
        android:background="@drawable/submit_button"
        app:layout_constraintTop_toBottomOf="@id/guideline11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="Noiton 보기"
        android:textColor="@color/white"
        android:textSize="@dimen/help_popup_font_small"
        android:fontFamily="@font/gmarketsans_medium"
        android:textAllCaps="false"
        android:paddingRight="@dimen/list_padding_top_bottom"
        android:paddingLeft="@dimen/list_padding_top_bottom"/>


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.45" />

    <TextView
        android:id="@+id/detail3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/guideline5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="더 자세한 사용방법은 아래 노션을 통해 확인 할 수 있습니다."
        android:textColor="#132743"
        android:textSize="@dimen/help_popup_font_small"
        android:fontFamily="@font/gmarketsans_medium"
        android:textAlignment="center"
        android:paddingRight="@dimen/list_padding_top_bottom"
        android:paddingLeft="@dimen/list_padding_top_bottom"/>


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.65" />

    <TextView

        android:id="@+id/detail4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/guideline6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="우주여행을 하는 동안 동료들과 '어디있니?'를 통해 더 편히 학습하시길 바라며, 저희 서비스에 많은 관심 부탁드니다."
        android:textColor="#132743"
        android:textSize="13sp"
        android:fontFamily="@font/gmarketsans_medium"
        android:textAlignment="center"
        android:paddingRight="@dimen/list_padding_top_bottom"
        android:paddingLeft="@dimen/list_padding_top_bottom"/>


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.76" />

    <TextView
        android:background="@drawable/submit_button"
        android:id="@+id/detail5"
        android:layout_width="@dimen/help_popup_gamil_width"
        android:layout_height="@dimen/checkBox"
        app:layout_constraintTop_toBottomOf="@id/guideline7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="문의사항: 42where@gmail.com"
        android:textColor="@color/white"
        android:textSize="@dimen/help_popup_font_small"
        android:textAlignment="center"
        android:fontFamily="@font/gmarketsans_medium"
        android:paddingRight="@dimen/list_padding_top_bottom"
        android:paddingLeft="@dimen/list_padding_top_bottom"/>


    <!--    &lt;!&ndash;    app:layout_constraintEnd_toStartOf="@id/submit"&ndash;&gt;-->
    <!--&lt;!&ndash;    app:layout_constraintTop_toBottomOf="@id/title"&ndash;&gt;-->

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.85" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.77" />

    <android.widget.Button
        android:id="@+id/submit"
        android:layout_width="@dimen/newgroup_button_width"
        android:layout_height="@dimen/edittext_layout_height"
        android:background="@drawable/submit_button"
        app:layout_constraintTop_toBottomOf="@id/guideline8"
        app:layout_constraintStart_toEndOf="@id/guideline9"
        android:paddingStart="@dimen/newgroup_button_padding"
        android:paddingEnd="@dimen/newgroup_button_padding"
        android:text="확인"
        android:textColor="@color/white"
        android:gravity="center"
        android:textSize="@dimen/newgroup_button_textsize"
        android:fontFamily="@font/gmarketsans_medium"/>


</androidx.constraintlayout.widget.ConstraintLayout>
