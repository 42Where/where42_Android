<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/center_popup_background"
    >

<!--    android:layout_width="wrap_content"-->
<!--    android:layout_height="wrap_content"-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05" />

    <TextView
        android:id="@+id/title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/guideline1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="어디있니?"
        android:textColor="#132743"
        android:textSize="@dimen/help_popup_font"
        android:fontFamily="@font/gmarketsans_bold" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.18" />

    <TextView
        android:id="@+id/middle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/guideline2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="안녕하세요! 42서울 친구 자리 찾기 서비스 '어디있니?'입니다."
        android:textColor="#132743"
        android:textSize="@dimen/help_popup_font_small"
        android:textAlignment="center"
        android:fontFamily="@font/gmarketsans_medium" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.277" />

    <TextView
        android:id="@+id/detail1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/guideline3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="'어디있니?' 서비스에서는 원하는 카뎃의 자리를 검색해 볼 수 있고, 카뎃들을 친구로 등록하여 여러명의 자리를 한 번에 모아볼 수 있습니다."
        android:textColor="#132743"
        android:textSize="@dimen/help_popup_font_small"
        android:textAlignment="center"
        android:fontFamily="@font/gmarketsans_medium"
        android:paddingRight="@dimen/list_padding_top_bottom"
        android:paddingLeft="@dimen/list_padding_top_bottom"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.51" />

    <Button
        android:id="@+id/noiton"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/submit_button"
        app:layout_constraintTop_toBottomOf="@id/guideline11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="Noiton 보기"
        android:textColor="@color/white"
        android:textSize="@dimen/font_medium_change"
        android:fontFamily="@font/gmarketsans_medium"
        android:textAllCaps="false"
        android:paddingRight="@dimen/list_padding_top_bottom"
        android:paddingLeft="@dimen/list_padding_top_bottom"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.45" />

    <TextView
        android:id="@+id/detail3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/guideline5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="더 자세한 사용방법은 아래 노션을 통해 확인 할 수 있습니다."
        android:textColor="#132743"
        android:textSize="@dimen/help_popup_font_small"
        android:fontFamily="@font/gmarketsans_medium"
        android:textAlignment="center"
        android:paddingRight="@dimen/list_padding_top_bottom"
        android:paddingLeft="@dimen/list_padding_top_bottom"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.69" />

    <TextView
        android:id="@+id/detail4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/guideline6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="우주여행을 하는 동안 동료들과 '어디있니?'를 통해 더 편히 학습하시길 바라며, 저희 서비스에 많은 관심 부탁드립니다."
        android:textColor="#132743"
        android:textSize="@dimen/help_popup_font_small"
        android:fontFamily="@font/gmarketsans_medium"
        android:textAlignment="center"
        android:paddingRight="@dimen/list_padding_top_bottom"
        android:paddingLeft="@dimen/list_padding_top_bottom"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.79" />

    <TextView
        android:background="@drawable/submit_button"
        android:id="@+id/detail5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/guideline7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="문의사항: 42where@gmail.com"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/help_popup_font_small"
        android:textAlignment="center"
        android:fontFamily="@font/gmarketsans_medium"
        android:paddingRight="@dimen/list_padding_top_bottom"
        android:paddingLeft="@dimen/list_padding_top_bottom"/>

</androidx.constraintlayout.widget.ConstraintLayout>

